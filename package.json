{
  "name": "things-scene-mqtt",
  "version": "0.1.13",
  "description": "An element providing a starting point for your own reusable Polymer elements.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "serve": "watchify src/*.js -o things-scene-mqtt.js src/index.js & polymer serve",
    "test": "./node_modules/.bin/mocha --require should --compilers js:babel-core/register test/**/test-*.js",
    "build": "browserify src/index.js -o things-scene-mqtt.js && uglifyjs things-scene-mqtt.js -o things-scene-mqtt-min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThingsElements/things-scene-mqtt.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ThingsElements/things-scene-mqtt/issues"
  },
  "homepage": "https://github.com/ThingsElements/things-scene-mqtt#readme",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "~7.2.0",
    "browserify": "~10.2.4",
    "chai": "^3.4.1",
    "gulp": "^3.8.5",
    "gulp-header-license": "^1.0.9",
    "mocha": "^2.3.4",
    "node-canvas": "Automattic/node-canvas",
    "should": "^8.0.2",
    "uglify-js": "^3.1.9",
    "watchify": "^3.3.1"
  },
  "dependencies": {
    "paho-mqtt": "^1.0.3"
  },
  "false": {}
}
